{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Launch",
  "colors": {
    "primary": "#000000",
    "light": "#ffffff",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "project-structure", "tech-stack", "quickstart"]
          },
          {
            "group": "Mobile App",
            "pages": [
              "mobile/environment-setup",
              "mobile/authentication",
              "mobile/onboarding",
              "mobile/file-structure",
              "mobile/components",
              "mobile/adding-screens"
            ]
          },
          {
            "group": "Authentication",
            "pages": [
              "authentication/backend-setup",
              "authentication/apple-signin",
              "authentication/google-signin",
              "authentication/email-signin"
            ]
          },
          {
            "group": "Payments",
            "pages": [
              "payments/index",
              {
                "group": "Stripe",
                "pages": [
                  "payments/stripe",
                  "payments/database-schema",
                  "payments/stripe-setup",
                  "payments/stripe-webhooks",
                  "payments/stripe-api",
                  "payments/paywall-best-practices"
                ]
              },
              "payments/revenuecat",
              "payments/superwall",
              "payments/paddle"
            ]
          },
          {
            "group": "AI Features",
            "pages": ["ai-features/overview"]
          },
          {
            "group": "Customization",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/your-repo/launch"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://github.com/your-repo/launch"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/launchhq",
      "github": "https://github.com/your-repo/launch",
      "linkedin": "https://linkedin.com/company/launchhq"
    }
  }
}
